{
  "minimum-stability": "beta",
  "prefer-stable": true,
  "require": {
    "craftcms/aws-s3": "^2.2.1",
    "craftcms/ckeditor": "4.1.0",
    "craftcms/cms": "5.4.0.1",
    "mmikkel/cp-field-inspect": "2.0.2",
    "nystudio107/craft-emptycoalesce": "^5.0.0",
    "nystudio107/craft-imageoptimize": "5.0.2",
    "nystudio107/craft-minify": "^5.0.0",
    "nystudio107/craft-typogrify": "5.0.1",
    "nystudio107/craft-vite": "5.0.1",
    "twig/intl-extra": "^3.9",
    "vlucas/phpdotenv": "^5.4.0",
    "yiisoft/yii2-redis": "^2.0.6"
  },
  "require-dev": {
    "craftcms/generator": "^2.0",
    "yiisoft/yii2-shell": "^2.0.3"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "allow-plugins": {
      "craftcms/plugin-installer": true,
      "yiisoft/yii2-composer": true
    },
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "8.2"
    }
  },
  "scripts": {
    "craft-update": [
      "@pre-craft-update",
      "@post-craft-update"
    ],
    "pre-craft-update": [],
    "post-craft-update": [
      "Composer\\Config::disableProcessTimeout",
      "@php craft install/check && php craft clear-caches/all --interactive=0 || exit 0",
      "@php craft install/check && php craft up --interactive=0 || exit 0"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php craft setup/welcome"
    ],
    "pre-update-cmd": "@pre-craft-update",
    "pre-install-cmd": "@pre-craft-update",
    "post-update-cmd": "@post-craft-update",
    "post-install-cmd": "@post-craft-update"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://composer.craftcms.com",
      "canonical": false
    }
  ]
}